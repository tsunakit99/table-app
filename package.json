	},
	"devDependencies": {
		"@eslint/eslintrc": "^3",
		"@tailwindcss/postcss": "^4",
		"@types/node": "^20.19.17",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"autoprefixer": "^10.4.21",
		"drizzle-kit": "^0.31.4",
		"eslint": "^8.57.1",
		"eslint-config-next": "^14.1.4",
		"postcss": "^8.5.6",
		"tailwindcss": "^3.4.17",
		"typescript": "^5",
		"wrangler": "^4.40.1"
